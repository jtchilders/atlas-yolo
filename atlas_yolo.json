{
   "data_handling": {
      "input_file_glob":               "/Users/jchilders/git/keras-yolo2/data/*.npz",
      "evt_per_file":                  10,
      "image_shape":                   [16,256,9600],
      "training_to_validation_ratio":  0.8,
      "shuffle":                       true,
      "classes":                       ["udjet", "sjet", "cjet", "bjet", "other"]
   },
   "model_pars": {
      "max_box_per_image":             5,
      "model_checkpoint_file":         "weights.{date}.{{epoch:02d}}-{{val_loss:.2f}}.hdf5"
   },
   "training": {
      "batch_size":                    2,
      "gridW":                         16,
      "gridH":                         40,
      "learning_rate":                 1e-4,
      "epochs":                        5,
      "verbose":                       1
   },
   "tensorboard": {
      "log_dir":                       "./logs",
      "histogram_freq":                1,
      "write_graph":                   true,
      "write_images":                  true,
      "write_grads":                   true,
      "embeddings_freq":               0
   }
}